<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude encrypted credentials from cloud backup for security -->
        <exclude domain="sharedpref" path="wifi_auto_login_prefs_encrypted"/>
        
        <!-- Include other app data -->
        <include domain="sharedpref" path="wifi_auto_login_prefs_regular"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Exclude encrypted credentials from device transfer for security -->
        <exclude domain="sharedpref" path="wifi_auto_login_prefs_encrypted"/>
        
        <!-- Include other app data -->
        <include domain="sharedpref" path="wifi_auto_login_prefs_regular"/>
    </device-transfer>
</data-extraction-rules>
